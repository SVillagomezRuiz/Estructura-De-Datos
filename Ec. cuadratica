#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double a, b, c;
    double discriminante, x1, x2;

    cout << "Ingrese el valor de a: ";
    cin >> a;
    cout << "Ingrese el valor de b: ";
    cin >> b;
    cout << "Ingrese el valor de c: ";
    cin >> c;

    if (a == 0) {
        cout << "No es una ecuacion cuadratica (a no puede ser 0)." << endl;
        return 0;
    }

    discriminante = b*b - 4*a*c;

    if (discriminante > 0) {
        x1 = (-b + sqrt(discriminante)) / (2*a);
        x2 = (-b - sqrt(discriminante)) / (2*a);
        cout << "Dos soluciones reales y distintas:" << endl;
        cout << "x1 = " << x1 << endl;
        cout << "x2 = " << x2 << endl;
    }
    else if (discriminante == 0) {
        x1 = -b / (2*a);
        cout << "Una solucion real doble:" << endl;
        cout << "x = " << x1 << endl;
    }
    else {
        double parteReal = -b / (2*a);
        double parteImaginaria = sqrt(-discriminante) / (2*a);
        cout << "Dos soluciones complejas:" << endl;
        cout << "x1 = " << parteReal << " + " << parteImaginaria << "i" << endl;
        cout << "x2 = " << parteReal << " - " << parteImaginaria << "i" << endl;
    }

    return 0;
}
